def main():
    f = open('C:\#dev\AOC - Solutions\AdventOfCode2022\Day 6\input.txt', 'r') 
    datastream = f.read()
    temp = set()
    print(datastream)
    for i in range(len(datastream)):
        chars = datastream[i:i+4]
        temp.add(chars)
        if len(set(chars)) == 4:
            return i + 4

print(main())